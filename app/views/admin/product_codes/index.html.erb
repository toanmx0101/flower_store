<p id="notice"><%= notice %></p>

<h1>Admin Product Codes</h1>

<table>
  <thead>
    <tr>
      <th>Store</th>
      <th>Status</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @admin_product_codes.each do |admin_product_code| %>
      <tr>
        <td><%= admin_product_code.store_id %></td>
        <td><%= admin_product_code.status %></td>
        <td><%= link_to 'Show', admin_product_code %></td>
        <td><%= link_to 'Edit', edit_admin_product_code_path(admin_product_code) %></td>
        <td><%= link_to 'Destroy', admin_product_code, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Admin Product Code', new_admin_product_code_path %>
