<!DOCTYPE html>
<html>

<head>
  <head>
  <%= csrf_meta_tags %>
    <title><%= full_title(yield(:title)) %></title>
  <link rel="stylesheet" edia="screen" href="/assets/bootstrap/css/bootstrap.min.css">
  <link rel="stylesheet" edia="screen" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.5.0/css/font-awesome.min.css">
  <link rel="stylesheet" edia="screen" href="https://cdnjs.cloudflare.com/ajax/libs/ionicons/2.0.1/css/ionicons.min.css">
  <link rel="stylesheet" edia="screen" href="/assets/dist/css/AdminLTE.min.css">
  <link rel="stylesheet" edia="screen" href="/assets/dist/css/skins/_all-skins.min.css">
  <link rel="stylesheet" edia="screen" href="/assets/plugins/iCheck/flat/blue.css">
  <link rel="stylesheet" edia="screen" href="/assets/plugins/morris/morris.css">
  <link rel="stylesheet" edia="screen" href="/assets/plugins/jvectormap/jquery-jvectormap-1.2.2.css">
  <link rel="stylesheet" edia="screen" href="/assets/plugins/datepicker/datepicker3.css">
  <link rel="stylesheet" edia="screen" href="/assets/plugins/daterangepicker/daterangepicker.css">
  <link rel="stylesheet" edia="screen" href="/assets/plugins/bootstrap-wysihtml5/bootstrap3-wysihtml5.min.css">

</head>

<% if logged_in? and current_user.admin? %>

<body class="hold-transition skin-blue sidebar-mini">
  <%= render 'layouts/admin/admin' %>
<script src="/assets/plugins/jQuery/jquery-2.2.3.min.js"></script>
<script src="https://code.jquery.com/ui/1.11.4/jquery-ui.min.js"></script>
<script>
  $.widget.bridge('uibutton', $.ui.button);
</script>
<script src="/assets/bootstrap/js/bootstrap.min.js"></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/raphael/2.1.0/raphael-min.js"></script>
<script src="/assets/plugins/morris/morris.min.js"></script>
<script src="/assets/plugins/sparkline/jquery.sparkline.min.js"></script>
<script src="/assets/plugins/jvectormap/jquery-jvectormap-1.2.2.min.js"></script>
<script src="/assets/plugins/jvectormap/jquery-jvectormap-world-mill-en.js"></script>
<script src="/assets/plugins/knob/jquery.knob.js"></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/moment.js/2.11.2/moment.min.js"></script>
<script src="/assets/plugins/daterangepicker/daterangepicker.js"></script>
<script src="/assets/plugins/datepicker/bootstrap-datepicker.js"></script>
<script src="/assets/plugins/bootstrap-wysihtml5/bootstrap3-wysihtml5.all.min.js"></script>
<script src="/assets/plugins/slimScroll/jquery.slimscroll.min.js"></script>
<script src="/assets/plugins/fastclick/fastclick.js"></script>
<script src="/assets/dist/js/app.min.js"></script>
<script src="/assets/dist/js/pages/dashboard.js"></script>
<script src="/assets/dist/js/demo.js"></script>

<%= javascript_include_tag 'admin/application',media: 'all', 'data-turbolinks-track': 'reload' %>

</body>
<% else %>

<body class="hold-transition login-page">


  <div class="login-box">
    <div class="login-logo">
      <a href="../../index2.html"><b>Admin</b>LTE</a>
    </div>
    <!-- /.login-logo -->
    <div class="login-box-body">
      <p class="login-box-msg">Sign in to start your session</p>
      <%= form_for(:session, url: login_path) do |f| %>
      <%= f.label :email %>
      <%= f.email_field :email, class: 'form-control', :required => true %>
      <%= f.label :password %>  
      <%= f.password_field :password, class: 'form-control', :required => true %>
       <div class="form-group"> 
        <div class="checkbox"> 
          <%= f.label :remember_me do %> 
            <%= f.check_box :remember_me %> Remember me 
          <% end %> 
        </div> 
      </div>
      <%= f.submit "Log in", class: "btn btn-primary" %>
    <% end %>
          <!-- /.social-auth-links -->

          <a href="#">I forgot my password</a><br>
          <a href="#" class="text-center">Register a new membership</a>

        </div>
        <!-- /.login-box-body -->
      </div>


      <!-- jQuery 2.2.3 -->


      <script>
        $(function () {
          $('input').iCheck({
            checkboxClass: 'icheckbox_square-blue',
            radioClass: 'iradio_square-blue',
      increaseArea: '20%' // optional
    });
        });
      </script>

            <% end %>

</html>
